<Window x:Class="Fire_HD_Google_Play_Installer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fire_HD_Google_Play_Installer"
        mc:Ignorable="d"
        Title="MainWindow" Height="215" Width="400">
    <Grid Width="400" Margin="0,0,0,-6">
        <Button Name="Install" Content="インストール開始" Margin="0,138,0,28" Click="Install_Click" HorizontalAlignment="Center" Width="80"/>
        <RadioButton Name="five" Content="Fire OS 5" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <RadioButton Name="six" Content="Fire OS 6" HorizontalAlignment="Left" Margin="10,30,0,0" VerticalAlignment="Top"/>
        <RadioButton Name="seven" Content="Fire OS 7" HorizontalAlignment="Left" Margin="10,50,0,0" VerticalAlignment="Top"/>
        <Label Name="adb_Log" Content="Adbのログが出ます" Margin="10,0,10,0" VerticalAlignment="Center"/>

    </Grid>
</Window>
